<epistudy>
  <title>&var_region;_&var_intv_type;_&var_scenario;</title>
  <region>&var_region;</region>
  <study_base_dir>&var_study_run_dir;</study_base_dir>
  <study_dir>&var_study_run_dir;</study_dir>
  <epistudy_config_base_dir>&var_cfg_base_dir;</epistudy_config_base_dir>
  <epistudy_config_dir>&var_cfg_dir;</epistudy_config_dir>
  <iql_template>&var_cfg_dir;/iql_&var_scenario;.&var_devel;template</iql_template> 
  <model_dir>&var_model_base_dir;/&var_region;</model_dir>
  <metadata_dir>&var_metadata_dir;</metadata_dir>

  <model_data>
    <edcfg_file>&var_cfg_dir;/intv_base_dataset_edcfg.xml</edcfg_file>
  </model_data>

  <epifast>
  <epifast_bin>/groups/NDSSL/projects/EpiFast/20160308_indemics_debug/EpiFast_Indemics_debug</epifast_bin>
    <socnet_path>&var_model_dir;/socialnet/EFIG6Bb</socnet_path>
    <num_nodes>4</num_nodes>
    <walltime>&var_epifast_walltime;</walltime>
    <Diagnosis> 
      <ModelVersion>2009</ModelVersion>
      <ProbSymptomaticToHospital>1.0</ProbSymptomaticToHospital>
      <ProbDiagnoseSymptomatic>1.0</ProbDiagnoseSymptomatic>
      <DiagnosedDuration>6</DiagnosedDuration>
    </Diagnosis>
    <transmissibility>0.000058</transmissibility>
    <asymptomatic_probability>0.3333</asymptomatic_probability>
    <asymptomatic_factor>0.5</asymptomatic_factor>
    <simulation_duration>&var_simulation_duration;</simulation_duration>
  </epifast>
  <system>
    <template_dir>templates</template_dir>  
    <dicex_base_dir>&var_dicex_base_dir;</dicex_base_dir>
    <indemics_dir>&var_dicex_base_dir;/&var_devel;/indemics_server</indemics_dir>
  </system>


  <data_engine>
    <engine_type>pgsa</engine_type>
    <dbsession>&var_region;_&var_intv_type;</dbsession>
    <walltime>&var_db_job_walltime;</walltime>
    <cluster_qsub>
      <group_list>&var_db_job_group;</group_list>
      <qsub_q>&var_db_job_queue;</qsub_q>
      <qsub_ppn>&var_db_job_ppn;</qsub_ppn>
    </cluster_qsub>
  </data_engine>

  <indemics_server>
    <max_threads>8</max_threads>
    <walltime>&var_indemics_server_job_walltime;</walltime>
    <cluster_qsub>
        <group_list>&var_indemics_server_job_group;</group_list>
        <qsub_q>&var_indemics_server_job_queue;</qsub_q>
        <qsub_ppn>&var_indemics_server_job_ppn;</qsub_ppn>
    </cluster_qsub>
  </indemics_server>
</epistudy>
